{
  "name": "MingleMunch",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "serve": "node dist/index.js",
    "watch": "npx tsc --watch",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "start": "concurrently \"npm run dev\" \"npm run shell\""
  },
  "engines": {
    "node": "18"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.0",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/cors": "^2.8.13",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "prisma": "^4.13.0"
  }
}
